# Compute paths
get_filename_component(MINIZIP_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(MINIZIP_INCLUDE_DIR "@CONF_INCLUDE_DIRS@" PARENT_SCOPE)
 
# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET @LIB_NAME@ AND NOT MINIZIP_BINARY_DIR)
  include("${MINIZIP_CMAKE_DIR}/MiniZipTargets.cmake")
endif()

set(BUILD_SHARED_LIBS @BUILD_SHARED_LIBS@)

if(MSVC)
  if(BUILD_SHARED_LIBS)
    get_target_property(LIB_LIB_FILENAME @LIB_NAME@ IMPORTED_IMPLIB_@CONFIG_TYPE@)
  else(BUILD_SHARED_LIBS)
    get_target_property(LIB_LIB_FILENAME @LIB_NAME@ IMPORTED_LOCATION_@CONFIG_TYPE@)
  endif(BUILD_SHARED_LIBS)
else()
  get_target_property(LIB_LIB_FILENAME @LIB_NAME@ IMPORTED_LOCATION_@CONFIG_TYPE@)
endif()

set(MINIZIP_LIBRARY ${LIB_LIB_FILENAME} PARENT_SCOPE)
#compatiblity
set(MINIZIP_LIBRARIES ${LIB_LIB_FILENAME} PARENT_SCOPE)

set(MINIZIP_FOUND TRUE)
